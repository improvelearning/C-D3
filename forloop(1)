int main(){
    int number1;
    int number2;
    int answer = 1;
    cout << "enter 2 numbers : \n";
    cin >> number1 >> number2;

    if ( number1 > number2)
    {
        for (int i = number1 ; i >= number2 ; i--)
        {
            answer *= i; //*= equal to int a = int a * int b
        }

        cout << "the answer is : " << answer << endl;      

    }
    else if(number2 > number1)
    {
        cout << "ERROR:first nunber is bigger than second number\n";
    }
    return 0;
}
