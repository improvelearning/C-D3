int main(){
    int length, height;
    int counter = 0;
    cout << "please enter the length and height : \n";
    cin >> length >> height;
 
    for (int i = 0 ; i < height ; i++)
    {
        for (int j = 1 ; j < length ; j++)
        {
            cout << "*";
        }
        cout << "*" << endl;
    }
    
    return 0;
}
